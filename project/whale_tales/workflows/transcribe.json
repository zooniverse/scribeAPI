{
  "name": "transcribe",
  "label": "Transcribe Workflow",
  "retire_limit": 3,
  "subject_request_scope": "active",
  "generates_subjects": true,
  "generates_subjects_for": "verify",
  "generates_subjects_after": 3,
  "generates_subjects_max": 10,
  "generates_subjects_method": "collect-unique",

  "tasks": {

    "transcribe_point": {
      "tool": "compositeTool",
      "tool_config": {
        "tools": {
          "em_valuation_date": {
            "tool": "dateTool",
            "tool_config": {},
            "label": "Date"
          },
          "em_valuation_amount": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Amount"
          }
        }
      },
      "instruction": "Transcribe this point.",
      "next_task": "entry_date"
    },
    "transcribe_rectangle": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Transcribe this rectangle.",
      "next_task": "entry_date"
    },
    "transcribe_text_row": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Transcribe this text row.",
      "next_task": "entry_date"

    },

    "entry_date": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the entry date?",
      "next_task": "page_number"
    },
    "page_number": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the page number?",
      "next_task": "location"
    },
    "location": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the location?",
      "next_task": "entry_text"
    },
    "entry_text": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Please transcribe the remainder of the highlighted entry.",
      "next_task": null
    },

    "location_name": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter the name of the location.",
      "next_task": null
    },
    "location_coordinates": {
      "tool": "compositeTool",
      "tool_config": {
        "tools": {
          "location_lat": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Latitude"
          },
          "location_lon": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Longitude"
          }
        }
      },
      "instruction": "Enter the geographic coordinates.",
      "next_task": null
    },
    "location_port": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter the name of the port.",
      "next_task": null
    },
    "location_other": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter a brief description of the location.",
      "next_task": null
    },




    "sea_ice_date": {
      "tool": "dateTool",
      "tool_config": {},
      "instruction": "Enter the date of the sea ice description",
      "next_task": null
    },

    "sea_ice_description": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Transcribe the description of the sea ice",
      "next_task": null
    },

    "sea_ice_other_description": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Include any additional comments regarding the sea ice.",
      "next_task": null
    },




    "weather_date": {
      "tool": "dateTool",
      "tool_config": {},
      "instruction": "What is the date of this entry?",
      "next_task": null
    },

    "weather_wind": {
      "tool": "compositeTool",
      "tool_config": {
        "tools": {
          "weather_date": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Wind direction/force"
          },
          "weather_sky": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Other description"
          }
        }
      },
      "instruction": "Enter the wind force/direction.",
      "next_task": "weather_wind2"
    },

    "weather_wind2": {
      "tool": "compositeTool",
      "tool_config": {
        "tools": {
          "weather_date2": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Wind direction/force"
          },
          "weather_sky2": {
            "tool": "textTool",
            "tool_config": {},
            "label": "Other description"
          }
        }
      },
      "instruction": "Enter the wind force/direction.",
      "next_task": null
    },


    "weather_sky": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter the sky/cloud description.",
      "next_task": null
    },

    "weather_precipitation": {
      "tool": "dateTool",
      "tool_config": {},
      "instruction": "Enter the precipitation description.",
      "next_task": null
    },

    "weather_sea": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Enter the description of the sea state.",
      "next_task": null
    },

    "weather_temperature": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter the temperature.",
      "next_task": null
    },

    "weather_pressure": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Enter the barometric pressure.",
      "next_task": null
    },

    "weather_other_description": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Enter the weather description as best you can.",
      "next_task": null
    },














    "transcribe_rectangle": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Transcribe this rectangle.",
      "next_task": "entry_date"
    },
    "transcribe_text_row": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "Transcribe this text row.",
      "next_task": "entry_date"
    },
    "entry_date": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the entry date?",
      "next_task": "page_number"
    },
    "page_number": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the page number?",
      "next_task": "location"
    },
    "location": {
      "tool": "textTool",
      "tool_config": {},
      "instruction": "What is the location?",
      "next_task": "entry_text"
    },
    "entry_text": {
      "tool": "textAreaTool",
      "tool_config": {},
      "instruction": "Please transcribe the remainder of the highlighted entry.",
      "next_task": null
    }



  }
}
