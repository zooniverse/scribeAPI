    {
      "name": "Record",
      "spec_fields": [
        {"name": "Mortgager", "repeats": false},
        {"name": "Street Address", "format": "address", "repeats": false},
        {"name": "Record Date", "format": "date", "format_options": {"range": [1850,1950]}, "repeats": false},
